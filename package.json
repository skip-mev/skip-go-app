{
  "name": "skip-go-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "chains:generate": "node ./src/scripts/generate-chains.cjs",
    "dev": "next dev",
    "dev:https": "next dev --experimental-https",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "postinstall": "patch-package && run-s chains:*",
    "start": "next start",
    "visdeps": "bash ./src/scripts/visdeps.sh",
    "update-registries": "npx npm-check-updates chain-registry @initia/initia-registry -u && npm i && npm run chains:generate",
    "update-skip-go": "npx npm-check-updates @skip-go/widget -u && npm i",
    "update-all": "npm run update-registries && npm run update-skip-go"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.17.10",
    "@heroicons/react": "^2.1.1",
    "@initia/initia-registry": "^1.0.6",
    "@next/third-parties": "^15.2.3",
    "@radix-ui/colors": "^3.0.0",
    "@skip-go/widget": "^3.14.18",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/react-query": "^5.18.1",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.53",
    "@types/react-dom": "^18.2.18",
    "@vercel/analytics": "^1.1.2",
    "@vercel/edge": "^1.2.2",
    "@vercel/edge-config": "^0.4.1",
    "autoprefixer": "^10.4.17",
    "chain-registry": "^2.0.133",
    "clsx": "^2.1.0",
    "next": "14.2.35",
    "next-seo": "^6.4.0",
    "nuqs": "^1.17.4",
    "postcss": "^8.4.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "viem": "^2.16.4",
    "wagmi": "^2.5.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.18.1",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "dependency-cruiser": "^16.2.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "npm-run-all2": "^6.1.2",
    "patch-package": "^8.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "typescript": "5.2.x"
  }
}
